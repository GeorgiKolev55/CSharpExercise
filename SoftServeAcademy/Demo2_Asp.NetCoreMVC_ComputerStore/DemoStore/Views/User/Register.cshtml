
@{
    ViewData["Title"] = "Register";
}

@model DemoStore.Dtos.UserDtoImport

<h2>Registration</h2>

<form method="post" asp-action="Register" asp-controller="User" asp-antiforgery="true">
    <div class="validation" asp-validation-summary="ModelOnly" />
    <div>
        <div>

            <label asp-for="Username">username</label><br />
            <input type="text" asp-for="Username" />
            <span asp-validation-for="Username" />
        </div>
        <div>
            <label asp-for="Password">password</label><br />
            <input asp-for="Password" />
            <span asp-validation-for="Password" />
        </div>

        <div>
            <input type="submit" value="Register" />
        </div>
    </div>
</form>

